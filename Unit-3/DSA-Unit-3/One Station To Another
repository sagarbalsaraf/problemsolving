function station1to2(N, A, S, D){
    // write code here
    let c=1;
    let tmp1=0, tmp2=0;
    for(let i=0;i<A.length;i++){
        if(A[i]==(S)){
         tmp1=i
        }
    }
        
    for(let i=0;i<A.length;i++){
        if(A[i]==D ){
            tmp2=i;
        }
    }
    for(let i=0;i<A.length;i++){
        if(A[i]==S && A[i+1]==D){
            c=1;
        }else{
           c=tmp2-tmp1
        }
    }
c==1 ? console.log(5*7): console.log(5*c*7)
}
