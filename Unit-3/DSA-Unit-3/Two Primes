function twoPrimes(a,b) {
    //write code here
    let c=0,c2=0;
    if((a || b) == 1){
        console.log("False")
    }else{
for(let i=1;i<=a;i++){
    if(a%i==0){
        c++;
    }
 
}
for(let j=1;j<=b;j++){
    if(b%j==0){
        c2++;
    }
}
}
 if(c==2 && c2==2){
     console.log("True")
 }else{
     console.log("False")
 }
}