function zTraversal(N,arr){
    //write code here
  let bag="";
  for(let i=0;i<N;i++){
    for(let j=0;j<N;j++){
      if(i==0){
      bag+=arr[i][j]+" ";
  }else if(i+j==N-1 && i!==0 && i!==N-1){
      bag+=arr[i][j]+" ";
  }else if(i==N-1){
      bag+=arr[i][j]+" ";
            }
        }
  }
    console.log(bag);
}
